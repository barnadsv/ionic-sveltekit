		"@sveltejs/kit": "^1.5.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.7",
		"classnames": "^2.3.2",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"flowbite": "^1.6.3",
		"flowbite-svelte": "^0.30.6",
		"konsta": "^1.0.2",
		"postcss": "^8.4.14",
